{
    "version":2,
    "builds":[
        {
            "src":"vikram/wsgi.py",
            "use":"@vercel/python",
            "config":{"maxLambdaSize":"15mb","runtime":"python3.9"}
        },
        {
            "src":"build_files.sh",
            "use":"@vercel/static-build",
            "config":{
                "distDir":"staticfiles_build"
            }
        }
    ],
    "routes":[
        {
            "src":"/static/(.*)",
            "use":"/static/$1"
        },
        {
            "src":"/(.*)",
            "use":"vikram/wsgi.py"
        }
    ]
}